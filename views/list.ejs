<!DOCTYPE html>
<html lang="en">

<% include header.ejs %>

<body>

<div id="content">

    <div class="container">

        <div class="row">

            <div class="span3">

                <div class="account-container">

                    <div class="account-details">

                        <span class="account-name"><%= userinfo.username %></span>

                        <span class="account-role"><%= userinfo.department %> | <%= userinfo.position %></span>

                        <span class="account-actions"><%= userinfo.email %></span>

                    </div> <!-- /account-details -->

                </div> <!-- /account-container -->

                <hr />

                <ul id="main-nav" class="nav nav-tabs nav-stacked">

                    <li  class="active">
                        <a href="/list">
                            <i class="icon-th-list"></i>
                            申请列表
                        </a>
                    </li>

                    <li>
                        <a href="/apply">
                            <i class="icon-pushpin"></i>
                            申请表单
                        </a>
                    </li>

                    <li>
                        <a href="/logout">
                            <i class="icon-lock"></i>
                            注销
                        </a>
                    </li>

                </ul>

                <hr />

                <div class="sidebar-extra">
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud.</p>
                </div> <!-- .sidebar-extra -->

                <br />

            </div> <!-- /span3 -->



            <div class="span9">

                <h1 class="page-title">
                    <i class="icon-th-list"></i>
                    会员数据申请列表
                </h1>

                <div class="widget widget-table">

                    <div class="widget-header">
                        <i class="icon-th-list"></i>
                        <h3>申请列表</h3>
                    </div> <!-- /widget-header -->

                    <div class="widget-content">

                        <table class="table table-striped table-bordered">
                            <thead>
                            <tr>
                                <th>#</th>
                                <th>申请编号</th>
                                <th>申请时间</th>
                                <th>申请字段</th>
                                <th>申请状态</th>
                                <!--<th>关闭申请</th>-->
                            </tr>
                            </thead>

                            <tbody>

                            <% for(var i = 0;i < userlist.length;i++){%>
                                <tr  onclick="parent.location='/details=<%=userlist[i].id_apply%>';">
                                    <td><%= i+1 %></td>
                                    <td><%=userlist[i].id_apply%></td>
                                    <td><%=userlist[i].date_apply%></td>
                                    <td><%=userlist[i].data_field_apply%></td>
                                    <td><%=userlist[i].states%></td>
                                    <!--<td class="action-td">
                                        <a href="javascript:;" class="btn btn-small"
                                                <% if(userlist[i].states == '已关闭') { %> disabled="" <% } else { %>  <% } %>
                                        >
                                            <i class="icon-remove"></i>
                                        </a>
                                    </td>-->
                                </tr>
                            <%} %>

                            </tbody>
                        </table>

                    </div> <!-- /widget-content -->

                </div> <!-- /widget -->


            </div> <!-- /span9 -->

        </div> <!-- /row -->

    </div> <!-- /container -->

</div> <!-- /content -->


<div id="footer">

    <div class="container">
        <hr />
        <p>&copy; 2012 Go Ideate.</p>
    </div> <!-- /container -->

</div> <!-- /footer -->

<!-- Le javascript================================================== -->
<!-- Placed at the end of the document so the pages load faster -->
<script src="./js/jquery-1.7.2.min.js"></script>
<script src="./js/bootstrap.js"></script>

</body>
</html>
